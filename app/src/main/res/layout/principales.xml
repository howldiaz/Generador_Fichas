        android:text="@string/sig"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/dValue"
        android:layout_width="25dp"
        android:layout_height="25dp"
