        app:layout_constraintTop_toBottomOf="@+id/vValue" />

    <TextView
        android:id="@+id/aValue"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="340dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cValue" />

    <SeekBar
        android:id="@+id/sbCarisma"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="40dp"
        android:max="10"
        android:progress="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sbVoluntad" />

    <SeekBar
        android:id="@+id/sbManipulacion"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="20dp"
        android:max="10"
        android:progress="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sbApariencia" />

    <TextView
        android:id="@+id/carisma"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="40dp"
        android:text="@string/carisma"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/voluntad" />

    <TextView
        android:id="@+id/apariencia"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
