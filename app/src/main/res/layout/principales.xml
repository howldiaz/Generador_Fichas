        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="20dp"
        android:max="10"
        android:progress="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sbInteligencia" />

    <TextView
        android:id="@+id/manipulacion"
        android:layout_width="wrap_content"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="@string/manipulacion"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/apariencia" />

    <SeekBar
        android:id="@+id/sbApariencia"
        android:layout_width="200dp"
        android:layout_height="25dp"
        android:layout_marginStart="120dp"
        android:layout_marginTop="20dp"
        android:max="10"
        android:progress="0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/sbCarisma" />

    <TextView
        android:id="@+id/fValue"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="340dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/mValue"
        android:layout_width="25dp"
        android:layout_height="25dp"
        android:layout_marginStart="340dp"
